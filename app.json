{
  "expo": {
    "name": "MoniPool",
    "slug": "monipool",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "monipool",
    "newArchEnabled": true,
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "expoDevTools": false
    },
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0E1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.monipool.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0A0E1A"
      },
      "package": "com.monipool.app",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro"
    }
  }
}